# 智能旅行对话Agent环境变量配置示例
# 复制此文件为 .env 并填入真实的API密钥

# ==================== 必需配置 ====================
# 豆包API密钥（必需）
DOUBAO_API_KEY=your_doubao_api_key_here

# ==================== 高德地图API密钥（可选） ====================
# 天气API密钥
AMAP_WEATHER_API_KEY=your_amap_weather_api_key_here

# 交通态势API密钥
AMAP_TRAFFIC_API_KEY=your_amap_traffic_api_key_here
AMAP_TRAFFIC_SECURITY_KEY=your_amap_traffic_security_key_here

# 导航API密钥
AMAP_NAVIGATION_API_KEY=your_amap_navigation_api_key_here

# POI搜索API密钥
AMAP_POI_API_KEY=your_amap_poi_api_key_here

# ==================== 可选配置 ====================
# Flask运行环境
FLASK_ENV=development

# 日志级别
LOG_LEVEL=INFO

# 缓存配置
CACHE_ENABLED=true
CACHE_DURATION=300

# MCP服务配置
MCP_TIMEOUT=5
MCP_RETRY_TIMES=3

